<h1> Create New User </h1>

<%= form_for @user do |f| %>

<%=f.label "user_name" %>
<%=f.text_field :username %>

<%=f.label "email" %>
<%=f.email_field :email %>

<%=f.label "password" %>
<%=f.password_field :password %>


<%=f.submit %>
<% end%>


<%= render partial: "layouts/errors", locals: { object: @user } %>
